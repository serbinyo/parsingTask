        }

        if ((email.indexOf("@")==-1 || email.indexOf("@")==0) || email.indexOf("@")==String(email).length) {
                alert("Неверный адрес!");
                return false;
        }
        if ((email.indexOf(".")==-1 || email.indexOf(".")==0) || email.indexOf(".")==String(email).length) {
                alert("Неверный адрес!");
                return false;
        }

        return true;
}
